{
  "name": "setup-edgedriver",
  "version": "1.0.3",
  "private": true,
  "description": "setup-edgedriver",
  "main": "lib/setup-edgedriver.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rml1997/setup-edgedriver.git"
  },
  "keywords": [
    "actions",
    "php",
    "setup"
  ],
  "author": "rml1997",
  "license": "GPL",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^4.0.0",
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^1.6.1",
    "semver": "^7.3.4",
    "typed-rest-client": "^1.8.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/semver": "^7.3.4",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run build && npm run format",
      "post-commit": "npm prune --production && git add node_modules/* && git commit -m \"Husky commit correct node modules\""
    }
  }
